<!doctype html>
<html>
  <head>
    <title>Welcome to <%=shopData.shopName%></title>
    <link rel="stylesheet"  type="text/css" href="/main.css" />
  </head>
  <body>
    <h1>List of users:</h1>
    <table width="500">
        <tr>
            <th>Username</th>
            <th>Firstname</th>
            <th>Lastname</th>
            <th>Email</th>
        </tr>
        <% users.forEach(function(user) { %>
            <tr>
                <td><%= user.username %></td>
                <td><%=user.firstname%></td>
                <td><%=user.lastname%></td>
                <td><%=user.email%></td>
            </tr>
          <% }) %>

    </table>
    </body>
</html>